<!DOCTYPE html>
{% if app.debug %}
	<html lang="en-us">
{% else %}
	<html lang="en-us" manifest="cache.manifest">
{% endif %}

<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1" />
	<meta name="apple-mobile-web-app-capable" content="yes" />
	<meta name="mobile-web-app-capable" content="yes" />
	<link rel="icon" sizes="196x196" href="{{ asset('img/ic_launcher_bigchub.png') }}" />
	<link rel="icon" sizes="128x128" href="{{ asset('img/ic_launcher_bigchub128.png') }}" />
	<link rel="apple-touch-icon" sizes="128x128" href="{{ asset('img/ic_launcher_bigchub128.png') }}" />
	<link rel="apple-touch-icon-precomposed" sizes="128x128" href="{{ asset('img/ic_launcher_bigchub128.png') }}" />
	<title>{% block title %}MFZ:RA Commander&#8217;s Handbook{% endblock %}</title>
	<link rel="stylesheet" href="{{ asset('themes/MFZApp.min.css') }}" />
	<link rel="stylesheet" href="{{ asset('themes/jquery.mobile.icons.min.css') }}" />
	<link rel="stylesheet" href="{{ asset('jq/jquery.mobile.structure-1.4.5.min.css') }}" />
	<script src="{{ asset('jq/jquery-1.11.1.min.js') }}"></script>
	<script src="{{ asset('jq/jquery.mobile-1.4.5.min.js') }}"></script>
	<script src="{{ asset('nosleep/NoSleep.min.js') }}"></script>
	<link rel="stylesheet" href="{{ asset('style.css') }}" />

	<meta name="robots" content="noindex,nofollow" />

	{% block htmlhead %}{% endblock %}
</head>
<body>

<noscript>
	<p>The MFZ:RA Commander&#8217;s Handbook requires a recent browser with Javascript enabled.</p>
</noscript>

<div data-role="panel" id="nav-panel" data-theme="c" data-position="left" data-position-fixed="true" data-display="reveal">
	<h2><a href="/#main-page">MFZ:RA Commander&#8217;s Handbook</a></h2>

	{% include 'nav.html.twig' %}
</div>
<script id="panel-init">
	// Load the side-panel var to the DOM / page
	$(document).one('pagebeforecreate', function () {
		$("#nav-panel").panel();
		$('.nav-listview').listview();
	});
</script>

<!-- ============================ -->

{% block main %}{% endblock %}

